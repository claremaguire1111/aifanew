{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "public": true,
  "outputDirectory": ".vercel/output/static",
  "images": {
    "sizes": [256, 640, 1200, 1920],
    "domains": ["aifilmacademy.io", "www.aifilmacademy.io"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**"
      }
    ]
  },
  "routes": [
    {
      "src": "/images/social/(.*)",
      "dest": "/images/social/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/images/support/(.*)",
      "dest": "/images/support/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}