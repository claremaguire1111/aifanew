{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "public": true,
  "outputDirectory": ".vercel/output/static",
  "images": {
    "sizes": [256, 640, 1200, 1920],
    "domains": ["aifilmacademy.io", "www.aifilmacademy.io"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**"
      }
    ]
  },
  "headers": [
    {
      "source": "/images/social/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/support/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}